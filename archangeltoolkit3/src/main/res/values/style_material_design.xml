<?xml version="1.0" encoding="utf-8"?>
<resources>

	<style name = "MDRipple">
		<item name = "android:textAllCaps">false</item>
		<item name = "textAllCaps">false</item>
		<item name = "rd_enable">true</item>
		<item name = "rd_inInterpolator">@android:anim/decelerate_interpolator</item>
		<item name = "rd_outInterpolator">@android:anim/decelerate_interpolator</item>
		<!--<item name = "rd_inInterpolator">@anim/interpolator_dec</item>-->
		<!--<item name = "rd_outInterpolator">@anim/interpolator_dec</item>-->
		<item name = "rd_maskType">rectangle</item>
		<item name = "rd_padding">0dp</item>
		<item name = "rd_delayClick">none</item>
		<item name = "rd_backgroundColor">@color/trans</item>
		<item name = "rd_backgroundAnimDuration">200</item>
		<item name = "rd_maxRippleRadius">match_view</item>
		<item name = "rd_rippleColor">@color/trans_12</item>
		<item name = "rd_rippleAnimDuration">300</item>
		<item name = "rd_rippleType">touch</item>
	</style>

	<style name = "MDSpinner">
		<item name = "spn_labelEnable">false</item>
		<item name = "spn_labelPadding">0dp</item>
		<item name = "spn_labelTextColor">@color/text_black</item>
		<item name = "spn_labelEllipsize">end</item>
		<item name = "spn_dividerHeight">0dp</item>
		<item name = "spn_dividerPadding">0dp</item>
		<item name = "spn_dividerAnimDuration">@android:integer/config_shortAnimTime</item>
		<item name = "spn_arrowColor">@color/trans</item>
		<item name = "spn_arrowSize">0dp</item>
		<item name = "spn_arrowPadding">0dp</item>
		<item name = "spn_popupItemAnimation">@anim/dialog_fade_in</item>
		<item name = "spn_popupItemAnimOffset">50</item>
	</style>

	<style name = "MDRipple.Light" parent = "MDRipple">
		<item name = "rd_rippleColor">@color/trans_white_12</item>
	</style>

	<style name = "MDFloatActionButton" parent = "MDRipple">
		<item name = "fab_backgroundColor">#FFDFDFDF</item>
		<item name = "fab_elevation">4dp</item>
		<item name = "rd_delayClick">none</item>
	</style>

	<style name = "MDButton" parent = "MDRipple">
		<!--<item name = "android:background">@drawable/bg_bt_raise</item>-->
		<item name = "rd_leftPadding">1dp</item>
		<item name = "rd_topPadding">1dp</item>
		<item name = "rd_rightPadding">1dp</item>
		<item name = "rd_bottomPadding">2dp</item>
		<item name = "rd_cornerRadius">2dp</item>
		<item name = "android:textColor">@color/text_black</item>
		<item name = "android:textSize">14sp</item>
		<item name = "rd_maxRippleRadius">48dp</item>
	</style>

	<style name = "MDButton.Flat" parent = "MDRipple">
		<item name = "android:background">@null</item>
		<item name = "rd_cornerRadius">2dp</item>
		<item name = "android:textColor">@color/text_black</item>
		<item name = "android:textSize">14sp</item>
		<item name = "rd_maxRippleRadius">48dp</item>
	</style>

	<style name = "MDButton.Flat.Light" parent = "MDRipple.Light">
		<item name = "android:background">@null</item>
		<item name = "rd_cornerRadius">2dp</item>
		<item name = "android:textColor">@color/text_black</item>
		<item name = "android:textSize">14sp</item>
		<item name = "rd_maxRippleRadius">48dp</item>
	</style>
</resources>